{
  "name": "webman-tech/laravel-console",
  "type": "library",
  "license": "MIT",
  "description": "Webman plugin webman-tech/laravel-console",
  "require": {
    "php": ">=7.4",
    "illuminate/console": ">=8",
    "illuminate/container": ">=8",
    "illuminate/events": ">=8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "workerman/webman-framework": "^1.5",
    "webman/console": "^1.2"
  },
  "autoload": {
    "psr-4": {
      "WebmanTech\\LaravelConsole\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WebmanTech\\LaravelConsole\\Tests\\": "tests",
      "": "tests",
      "app\\": "tests\\app"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit --testdox --no-interaction"
  }
}
